{
  "manifest_version": 3,
  "name": "Tutor Log Autofill",
  "version": "0.1.0",
  "description": "Autofill Tutor Log and persist client totals.",
  "permissions": ["storage"],
  "host_permissions": [
    "https://www.arbortutors.com/*",
    "https://arbortutors.wufoo.com/*",
    "https://*.wufoo.com/*"

  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Tutor Log Clients",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://arbortutors.wufoo.com/*",
        "https://*.wufoo.com/*"

      ],
      "js": ["arbor_script.js", "form_reader.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}
